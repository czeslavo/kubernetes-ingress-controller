{
  "gomod": {
    "enabled": false
  },
  "kustomize": {
    "enabled": false
  },
  "github-actions": {
    "enabled": false
  },
  "dockerfile": {
    "enabled": false
  },
  "automerge": false,
  "registryAliases": {
    "kong": "https://charts.konghq.com"
  },
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^.github/test_dependencies.yaml$"],
      "matchStrings": [
        "# renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?) versioning=(?<versioning>.*?)$\n.+'(?<currentValue>.*?)'$"
      ]
    }
  ]
}
