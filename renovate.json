{
  "gomod": {
    "enabled": false
  },
  "kustomize": {
    "enabled": false
  },
  "github-actions": {
    "enabled": false
  },
  "dockerfile": {
    "enabled": false
  },
  "automerge": false,
  "separateMinorPatch": true,
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^.github/test_dependencies.yaml$"],
      "matchStrings": [
        "#\\s+renovate:\\s+datasource=(?<datasource>.*)\\s+depName=(?<depName>.*)\\s+versioning=(?<versioning>.*)\\n.+'(?<currentValue>.*)'"
      ]
    }
  ],
  "customDatasources": {
    "gke-rapid": {
      "defaultRegistryUrlTemplate": "https://raw.githubusercontent.com/czeslavo/gke-renovate-datasource/main/static/rapid.json",
      "format": "json"
    }
  }
}
